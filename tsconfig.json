{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es5",
    "module": "commonjs",
    "lib": ["dom", "es2015", "es2016", "es2017"],
    "allowJs": true,
    "jsx": "react-jsx",
    "sourceMap": true,
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "paths": {
      "@root/*": ["src/*"],
      "@utils/*": ["src/utils/*"],
      "@components/*": ["src/components/*"],
      "@pages/*": ["src/pages/*"],
      "@views/*": ["src/views/*"],
      "@router/*": ["src/router/*"],
      "@styles/*": ["src/styles/*"],
      "@layout/*": ["src/layout/*"],
      "@config/*": ["config/*"],
      "@imgs/*": ["src/static/imgs/*"],
      "@icons/*": ["src/static/icons/*"],
      "@static/*": ["src/static/*"],
      "@constants/*": ["src/constants/*"],
      "@mytypes/*": ["src/types/*"],
      "@libs/*": ["src/libs/*"]
    },
    "types": ["react/next", "react-dom/next", "node", "webpack-env"]
  }
}
